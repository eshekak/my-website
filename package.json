{
  "devDependencies": {
    "husky": "^4.2.3",
    "karma": "4.4.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-cljs-test": "0.1.0",
    "karma-junit-reporter": "2.0.1",
    "shadow-cljs": "2.8.83"
  },
  "dependencies": {
    "express": "^4.17.1",
    "highlight.js": "9.15.10",
    "lint-staged": "^10.0.7",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-highlight.js": "1.0.7",
    "serve": "^11.3.0",
    "zprint-clj": "^0.8.0"
  },
  "scripts": {
    "serve": "node ./serve.js",
    "format": "zprint-clj -i './**/*.{clj,cljs,cljc,edn}' -o ./"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{clj,cljs,cljc,edn}": [
      "npm run format"
    ]
  }
}
