{
  "devDependencies": {
    "@wdio/cli": "^5.22.0",
    "@wdio/local-runner": "^5.21.0",
    "@wdio/mocha-framework": "^5.18.7",
    "@wdio/spec-reporter": "^5.18.7",
    "@wdio/sync": "^5.20.1",
